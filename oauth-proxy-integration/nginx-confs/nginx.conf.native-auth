worker_processes  1;

error_log /dev/stderr notice;

events {
    worker_connections 1024;
}

http {
    server {
        listen 443 ssl;
        ssl_certificate     /etc/pki/proxy.pem;
        ssl_certificate_key /etc/pki/proxy.key;

        location / {
            proxy_pass http://127.0.0.1:3000;
        }
    }
}
